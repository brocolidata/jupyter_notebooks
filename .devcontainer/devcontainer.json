{
  "name": "Jupyter Lab",
  "dockerComposeFile": "../docker/docker-compose.yml",
  "service": "jupyter_notebooks",
  "workspaceFolder": "/jupyter_lab/notebooks",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
		"ms-python.python",
		"ms-toolsai.vscode-jupyter-cell-tags",
		"redhat.vscode-yaml"
	]
    }
  }
}